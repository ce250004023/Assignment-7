#include <iostream>
using namespace std;

void sort(char arr[], int sz)
{
  for (int i = 1; i < sz; i++)
  {
    for (int j = 0; j < sz-i; j++)
    {
      if (arr[j] > arr[j+1])
      {
        int temp = arr[j];
        arr[j] = arr[j+1];
        arr[j+1] = temp;
      }
    }
  }
}


int main() {
    int SIZE;
    cout<<"Enter the size of Array: ";
    cin>>SIZE;
    char arr[SIZE];
    for(int i = 0; i<SIZE;++i){
        cout<<"Enter a character: ";
        cin>>arr[i];
    }
    sort(arr, SIZE);
    cout<<"[";
    for (int i = 0; i < SIZE; i++){
        cout << arr[i] << ",";
    }
    cout<<"]";

    return 0;
}
